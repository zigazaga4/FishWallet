{
  "name": "fishWallet",
  "version": "1.0.0",
  "description": "Electron application with React, TypeScript, and Tailwind CSS",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build:sandbox && node scripts/dev.mjs",
    "postinstall": "electron-rebuild",
    "build": "npm run build:main && npm run build:renderer && npm run build:sandbox",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build:sandbox": "vite build --config vite.sandbox.config.ts",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "pack": "npm run build && electron-builder --config electron-builder.config.js --dir",
    "dist": "npm run build && electron-builder --config electron-builder.config.js",
    "dist:linux": "npm run build && electron-builder --config electron-builder.config.js --linux",
    "dist:win": "npm run build && electron-builder --config electron-builder.config.js --win",
    "dist:mac": "npm run build && electron-builder --config electron-builder.config.js --mac",
    "dist:all": "npm run build && electron-builder --config electron-builder.config.js --linux --win --mac",
    "dist:linux-win": "npm run build && electron-builder --config electron-builder.config.js --linux --win"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "author": "FishWallet Team",
  "license": "ISC",
  "homepage": "https://fishwallet.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishwallet/fishwallet"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@langchain/anthropic": "^1.3.10",
    "@langchain/core": "^1.1.15",
    "@mendable/firecrawl-js": "^4.11.3",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/three": "^0.182.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.6.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "electron-log": "^5.4.3",
    "framer-motion": "^12.27.5",
    "langchain": "^1.2.10",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "openai": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.182.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "get-port": "^7.1.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  }
}
